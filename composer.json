{
  "name": "heygeeks/secure-file-transfer",
  "description": "Secure file transfer package for Laravel",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/http-client": "^6.0|^7.0",
    "firebase/php-jwt": "^6.10",
    "illuminate/cache": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/log": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Heygeeks\\SecureFileTransfer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Heygeeks\\SecureFileTransfer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Heygeeks\\SecureFileTransfer\\Providers\\SecureFileTransferServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
